	xsi:schemaLocation="http://schema.pivotal.io/gemfire/cache http://schema.pivotal.io/gemfire/cache/cache-8.1.xsd"
	version="8.1" is-server="true">
	<gateway-sender id="wfnEmployees_SENDER"
		remote-distributed-system-id="${REMOTE_DISTRIBUTED_SYSTEM_ID}" parallel="true" enable-persistence="true"
		disk-store-name="adp-gateway-store" dispatcher-threads="1" />

		disk-store-name="adp-gateway-store" dispatcher-threads="1" />
	<gateway-receiver start-port="${GATEWAY_RECEIVER_START_PORT}" end-port="${GATEWAY_RECEIVER_END_PORT}" />
	
	<cache-server port="${CACHE_SERVER_PORT}" bind-address="${CACHE_SERVER_BIND_ADDRESS}" />
	
	<disk-store name="adp-gemfire-store">
		<disk-dirs>
			<disk-dir>data</disk-dir>
		</disk-dirs>
	</disk-store>
	<disk-store name="adp-pdx-store" max-oplog-size="64">
		<disk-dirs>
			<disk-dir>pdx</disk-dir>
		</disk-dirs>
	</disk-store>
	<disk-store name="adp-gateway-store">
		<disk-dirs>
			<disk-dir>gateway</disk-dir>
		</disk-dirs>
	</disk-store>
	<disk-store name="adp-security-store" max-oplog-size="16">
		<disk-dirs>
			<disk-dir>security</disk-dir>
		</disk-dirs>
	</disk-store>
	<pdx read-serialized="true" persistent="true" disk-store-name="adp-pdx-store" />
	<region name="__gemusers">
		<region-attributes scope="distributed-ack"
			enable-gateway="true" data-policy="persistent-replicate"
			statistics-enabled="true" disk-store-name="adp-security-store"
			gateway-sender-ids="DC2">
			<cache-loader>
				<class-name>com.adp.gemfire.security.DeclaredUserCacheLoader</class-name>
				<parameter name="users">
					<string>gatewayNA pWu9206feTju0Mi6pQ9tThthHT4 secadmin design read write| admin 1b2Cyi3+MhQLLtgRxpYX1sPC9zU= secadmin</string>
				</parameter>
			</cache-loader>
		</region-attributes>
	</region>
	<region name="currentPayPeriod">
		<region-attributes enable-gateway="true"
			data-policy="persistent-partition" statistics-enabled="true"
			disk-store-name="adp-gemfire-store" gateway-sender-ids="currentPayPeriod_SENDER">
			<partition-attributes redundant-copies="1" />
			<eviction-attributes>
				<lru-heap-percentage action="overflow-to-disk" />
			</eviction-attributes>
		</region-attributes>
	</region>
	<region name="testRegionP">

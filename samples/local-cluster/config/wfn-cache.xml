	<gateway-sender id="DC2" remote-distributed-system-id="${REMOTE_DISTRIBUTED_SYSTEM_ID}"
		enable-persistence="true" disk-store-name="adp-gateway-store"
		dispatcher-threads="1" />
	<gateway-sender id="wfnVpdOrgMapping_SENDER"

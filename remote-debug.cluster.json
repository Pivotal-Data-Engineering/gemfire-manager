{
    "global-properties":{
        "gemfire": "${GEMFIRE}",
        "java-home" : "${JAVA_HOME}",
        "locators" : "localhost[10000],localhost[11000]",
        "cluster-home" : ".",
        "security-manager": "io.pivotal.pde.gemfire.dynamic.security.DynamicSecurityManager",
        "security-peer-password" : "passw0rd",
        "security-admin-password" : "opensesame",
        "security-username" : "gfpeer",
        "security-password" : "passw0rd",
        "security-disk-store-dir" : "data/security",
        "classpath": "../../target/gemfire-dynamic-security-1.1.0.jar",
        "distributed-system-id": 1
    },
   "locator-properties" : {
        "jmx-manager" : "true",
        "log-level" : "config",
        "statistic-sampling-enabled" : "true",
        "log-file-size-limit" : "10",
        "log-disk-space-limit" : "100",
        "archive-file-size-limit" : "10",
        "archive-disk-space-limit" : "100",
        "enable-network-partition-detection" : "true",
        "statistic-archive-file" : "locator.gfs",
        "log-file" : "locator.log",
        "jvm-options" : ["-Xmx256m","-Xms256m", "-XX:+UseConcMarkSweepGC", "-XX:+UseParNewGC","-Xdebug", "-Xnoagent","-Xrunjdwp:transport=dt_socket,address=8000,server=y"]
    },
   "datanode-properties" : {
        "conserve-sockets" : false,
        "log-level" : "config",
        "membership-port-range" : "10901-10999",
        "statistic-sampling-enabled" : "true",
        "log-file-size-limit" : "10",
        "log-disk-space-limit" : "100",
        "archive-file-size-limit" : "10",
        "archive-disk-space-limit" : "100",
        "statistic-archive-file" : "server.gfs",
        "log-file" : "server.log",
        "enable-network-partition-detection" : "true",
        "gemfire.ALLOW_PERSISTENT_TRANSACTIONS" : "true",
        "jvm-options" : ["-Xmx512m","-Xms512m","-Xmn64m", "-XX:+UseConcMarkSweepGC", "-XX:+UseParNewGC", "-XX:CMSInitiatingOccupancyFraction=85"]
    },
    "hosts": {
        "localhost" : {
            "host-properties" :  {
             },
             "processes" : {
                "locator1" : {
                    "type" : "locator",
                    "port" : 10000,
                    "jmx-manager-port" : 10099,
                    "http-service-port" : 10070,
                    "jmx-manager-start" : "true"
                 },
                "datanode1" : {
                    "type" : "datanode",
                    "tcp-port" : 10001,
                    "server-port" : 10100
                }
             }
        }
    }
}
